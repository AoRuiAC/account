# 启动mysql
docker run -d --name mysql -p 3306:3306 -v /my-docker/mysql/data:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=123456 mysql

# 启动redis
redis.conf:
daemonize no
port 6379
bind 0.0.0.0
cluster-enabled yes
cluster-config-file nodes.conf
cluster-node-timeout 5000
appendonly yes

docker run -d --name redis -p 6379:6379 \
-v /my-docker/redis/node-1/conf/redis.conf:/etc/redis/redis.conf \
-v /my-docker/redis/node-1/data:/data redis